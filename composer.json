{
  "name": "hilaryosborne/wordpress-composer",
  "description": "",
  "keywords": ["WordPress"],
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Hilary Osborne"
    },
    {
      "name": "Andris Trieb"
    }
  ],
  "type": "project",
  "repositories":[
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "advanced-custom-fields/advanced-custom-fields-pro",
        "version": "5.5.13",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://connect.advancedcustomfields.com/index.php?p=pro&a=download"
        },
        "require": {
          "philippbaschke/acf-pro-installer": "^1.0",
          "composer/installers": "^1.0"
        }
      }
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\" : "app/"
    },
     "files": [
       "app/Session/helpers.php",
       "app/Cache/helpers.php"
     ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "require": {
    "php": ">=5.6.4",
    "johnpbloch/wordpress-core-installer": "~0.2",
    "johnpbloch/wordpress-core": "^4.1.1",
    "twig/twig": "^2.0",
    "vlucas/phpdotenv": "^2.4",
    "mailgun/mailgun-php": "^2.3",
    "php-http/curl-client": "^1.7",
    "guzzlehttp/psr7": "^1.4",
    "advanced-custom-fields/advanced-custom-fields-pro": "*",
    "wpackagist-plugin/better-search-replace" : "*",
    "wpackagist-plugin/mailgun" : "*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.3.*"
  },
  "extra":{
    "installer-paths":{
      "plugins/{$name}/": ["type:wordpress-plugin"]
    },
    "wordpress-install-dir": {
      "johnpbloch/wordpress-core": "public"
    }
  }
}
