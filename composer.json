{
  "name": "hilaryosborne/wordpress-composer",
  "description": "",
  "keywords": ["WordPress"],
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Hilary Osborne"
    },
    {
      "name": "Andris Trieb"
    }
  ],
  "type": "project",
  "repositories":[
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "wp-premium/advanced-custom-fields-pro",
        "type": "wordpress-plugin",
        "version": "1.0.0",
        "source": {
          "url": "https://github.com/wp-premium/advanced-custom-fields-pro",
          "type": "git",
          "reference": "master"
        }
      }
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\" : "app/"
    },
     "files": [
       "app/Session/helpers.php"
     ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "require": {
    "php": ">=5.6.4",
    "johnpbloch/wordpress-core-installer": "~0.2",
    "johnpbloch/wordpress-core": "^4.1.1",
    "twig/twig": "^2.0",
    "vlucas/phpdotenv": "^2.4",
    "mailgun/mailgun-php": "^2.3",
    "php-http/curl-client": "^1.7",
    "guzzlehttp/psr7": "^1.4",
    "wp-premium/advanced-custom-fields-pro": "*",
    "wpackagist-plugin/better-search-replace" : "*",
    "wpackagist-plugin/mailgun" : "*",
    "sentry/sentry": "^1.7"
  },
  "require-dev": {
    "phpunit/phpunit": "5.3.*"
  },
  "extra":{
    "installer-paths":{
      "plugins/{$name}/": ["type:wordpress-plugin"]
    },
    "wordpress-install-dir": {
      "johnpbloch/wordpress-core": "public"
    }
  }
}
